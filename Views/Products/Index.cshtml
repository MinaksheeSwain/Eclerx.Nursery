<div class="container">
    <div class="row">
        <div class="col-4">
            <table class=" table table-striped table-hover">
                <tr>
                    <th>Id</th>
                    <th>TreeName</th>
                    <th>SKU-No</th>
<th>Price</th>
                </tr>
                @foreach(var nu in Model)
            {
                <tr>
                    <td>@nu.Id</td>
                    <td>@nu.TreeName</td>
                    <td>@nu.SKU_No-No</td>
                    <td>@nu.Price</td>
                </tr>
            }
            </table>
         
        </div>
    </div>
</div>